{
  "name": "vue2.7",
  "private": false,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "npm run prebuild && vite build",
    "build-demo": "npm run prebuild && vite build -- project=demo",
    "build-demo01": "npm run prebuild && vite build -- project=demo01",
    "build-eventCenter": "npm run prebuild && vite build -- project=eventCenter",
    "build-eventList": "npm run prebuild && vite build -- project=eventList",
    "build-taskWork": "npm run prebuild && vite build -- project=taskWork",
    "build-videoPatrol": "npm run prebuild && vite build -- project=videoPatrol",
    "build-alarmConfiguration": "npm run prebuild && vite build -- project=alarmConfiguration",
    "build-energyManagement": "npm run prebuild && vite build -- project=energyManagement",
    "preview": "vite preview",
    "lint": "npm run lint:eslint &&  npm run lint:stylelint",
    "lint:eslint": "eslint \"src/**/*.{vue,js,jsx}\" --fix",
    "lint:stylelint": "stylelint \"src/**/*.{vue,css,less,scss}\" --fix --custom-syntax postcss-less",
    "clean": "rimraf ./dist && mkdir dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "echarts": "^5.4.0",
    "element-ui": "^2.15.12",
    "ioc-module-component": "^0.0.33",
    "ioc-module-components": "^0.0.26",
    "js-md5": "^0.7.3",
    "moment": "^2.29.1",
    "pinia": "^2.0.14",
    "vue": "^2.7.5",
    "vue-router": "^3.6.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue2": "^1.1.2",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.19",
    "postcss-html": "^1.4.1",
    "postcss-less": "^6.0.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.1",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "vite": "^2.9.9",
    "vue-eslint-parser": "^9.0.3"
  },
  "browserslist": [
    "defaults"
  ],
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.vue": [
      "stylelint --fix",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{less,css}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
